{
  "name": "@hasezoey/verify-typegoose-issues",
  "version": "0.0.1",
  "description": "Bootstrap TS Project All Files needed for TS",
  "main": "lib/app.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "run:directly": "ts-node ./src/test.ts",
    "run": "yarn run build && node lib/test.js",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "doc": "typedoc --out ./doc ./src --mode modules",
    "clean": "rm -rf ./doc ./lib ./coverage ./nyc_output"
  },
  "repository": "none",
  "author": "hasezoey",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/source-map-support": "^0.5.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@typegoose/auto-increment": "^0.6.0",
    "@typegoose/typegoose": "~8.0.0-beta.9",
    "mongoose": "^5.12.10",
    "source-map-support": "^0.5.19"
  }
}
