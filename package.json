{
  "name": "@typegoose/verify-typegoose-issues",
  "version": "0.0.1",
  "description": "Bootstrap TS Project All Files needed for TS",
  "main": "lib/app.js",
  "engines": {
    "node": ">=14.17.0"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "run:directly": "ts-node ./src/test.ts",
    "run": "yarn run build && node lib/test.js",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint -c ./.eslintrc.js './**/*.{js,ts}'",
    "doc": "typedoc --out ./doc ./src --mode modules",
    "clean": "rm -rf ./doc ./lib ./coverage ./nyc_output"
  },
  "repository": "none",
  "author": "hasezoey",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "~14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@typegoose/auto-increment": "^3.0.0",
    "@typegoose/typegoose": "11.0.0",
    "mongoose": "7.0.3"
  }
}
