{
  "name": "@typegoose/verify-typegoose-issues",
  "version": "0.0.1",
  "description": "Bootstrap TS Project All Files needed for TS",
  "main": "lib/app.js",
  "engines": {
    "node": ">=14.17.0"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "run:directly": "ts-node ./src/test.ts",
    "run": "yarn run build && node lib/test.js",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint -c ./.eslintrc.js './**/*.{js,ts}'",
    "doc": "typedoc --out ./doc ./src --mode modules",
    "clean": "rm -rf ./doc ./lib ./coverage ./nyc_output"
  },
  "repository": "none",
  "author": "hasezoey",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "~14.18.63",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@typegoose/auto-increment": "^3.5.0",
    "@typegoose/typegoose": "11.5.1",
    "mongoose": "7.5.4"
  }
}
